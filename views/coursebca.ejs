<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <link rel="icon" href="/favicon.ico" />

    <title>Admission Portal</title>

    <!-- Fontawesome  link -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css"
      integrity="sha512-1sCRPdkRXhBV2PBLUdRb4tMg1w2YPf37qatUFeS7zlBy7jJI8Lf4VHwWfZZfpXtYSLy85pkm9GaYVYMfw5BC1A=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />

    <!-- Custom styles for this template -->
    <link href="/assets/css/styles.css" rel="stylesheet" />

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
      crossorigin="anonymous"
    />

    <!-- <link href="/public/assets/js/bootstrap.bundle.js" rel="stylesheet"> -->
    <script>
      function myFunction() {
        var x = document.getElementById("myInput");
        if (x.type === "password") {
          x.type = "text";
        } else {
          x.type = "password";
        }
      }
    </script>
  </head>

  <body>
    <%- include("./header.ejs") %>

  <br>
    <section id="registration" style="padding-top: 5%;">
      <div class="container">
        <h1 class="text-center pt-5">BCA Registration Form</h1>
        <h1 class="text-center text-uppercase pb-2" style="color: #ed5a25">
          Do Register!
        </h1>
        <p class="text-center" style="color: #fff; background-color: #35b9dd">
          In Case any query, Kindly Contact: 7974978812
        </p>
        <div class="row">
          <div class="col-lg-6">
            <!-- Slider Div start -->
            <div class="carousel slide" data-bs-ride="carousel" id="a">
              <div class="carousel-indicators">
                <button
                  type="button"
                  data-bs-target="#a"
                  data-bs-slide-to="0"
                  class="active"
                  aria-current="true"
                ></button>
                <button
                  type="button"
                  data-bs-target="#a"
                  data-bs-slide-to="1"
                  class=""
                ></button>
                <button
                  type="button"
                  data-bs-target="#a"
                  data-bs-slide-to="2"
                  class=""
                ></button>
              </div>
             
              
              <div class="carousel-inner">
                <div
                  class="carousel-item carousel-item-next carousel-item-start"
                  data-bs-interval="4000"
                >
                  <img
                    src="/assets/images/registration_form1.jpg"
                    width="100%"
                    height="700px"
                    alt="..."
                    style="background-position: center"
                  />
                  <div class="carousel-caption"></div>
                  <!--carousel-caption-->
                </div>

                <div class="carousel-item" data-bs-interval="2000">
                  <img
                    src="/assets/images/registration_form2.jpg"
                    width="100%"
                    height="700px"
                    alt="..."
                    style="background-position: center"
                  />
                  <div class="carousel-caption"></div>
                  <!--carousel-caption-->
                </div>

                <div
                  class="carousel-item active carousel-item-start"
                  data-bs-interval="2000"
                >
                  <img
                    src="/assets/images/registration_form3.jpg"
                    width="100%"
                    height="700px"
                    alt="..."
                    style="background-position: center"
                  />
                  <div class="carousel-caption"></div>
                  <!--carousel-caption-->
                </div>
              </div>
              <!--carousel-inner-->

              <button
                class="carousel-control-prev"
                type="button"
                data-bs-target="#a"
                data-bs-slide="prev"
              >
                <span class="carousel-control-prev-icon"></span>
              </button>
              <button
                class="carousel-control-next"
                type="button"
                data-bs-target="#a"
                data-bs-slide="next"
              >
                <span class="carousel-control-next-icon"></span>
              </button>
            </div>
          </div>
          <!-- Slider Div end -->
          <div class="col-lg-6">
            <form
              action="/register/bca"
              method="post"
              enctype="multipart/form-data"
            >
              <div class="mb-3 mt-1">
                 
                <label class="mb-2"><%=d.username%></label>
                <input required
                  type="text"
                  name="name"
                  value=""
                  class="form-control enter_name"
                  placeholder="Enter name"
                  style="border: 1px solid #35b9dd"
                />
              </div>
             
              
              <div class="text-center">
                <button required
                  class="btn my-4"
                  style="background-color: #35b9dd; color: #fff"
                  type="submit"
                >
                  Submit
                </button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </section>
    <%- include("./footer.ejs") %>
    
    <!-- Container-fluid end -->
    <!-- Footer Section end -->
    <!-- Bootstrap core JavaScript
  ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <!-- JavaScript Bundle with Popper -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
      crossorigin="anonymous"
    ></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>

    <script src="js/jquery.min.js"></script>
    <script src="js/popper.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.validate.min.js"></script>
    <script src="js/main.js"></script>
  </body>
</html>
